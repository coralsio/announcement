{
    "name": "corals/announcement",
    "description": "Announcement",
    "keywords": [
        "Announcement"
    ],
    "homepage": "https://github.com/coralsio/announcement",
    "license": "MIT",
    "authors": [
        {
            "name": "corals",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Corals\\Modules\\Announcement\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "laravel/pint": "^1.0"
    },
    "require": {
        "laravel-notification-channels/twilio": "^3.1"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Corals\\Modules\\Announcement\\AnnouncementServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
